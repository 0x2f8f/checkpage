{% trans_default_domain 'FOSUserBundle' %}

<div class="col-lg-3">
    <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register" role="form">
        <div class="form-group">
            {{ form_widget(form) }}
        </div>
        <div>
            <input type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-default" />
            <a class="btn" href="{{ path('fos_user_security_login') }}">Войти</a>
        </div>
    </form>
</div>
